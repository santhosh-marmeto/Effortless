
<button onclick="append()">Button View</button>
<div id="appendContainer">

</div>
<script>
    function append(){
        fetch("/?section_id=template--17729771012316__custom_usp_HJUx4t")
        .then(response=>response.text())
        .then((responseText) => {
            const html = new DOMParser().parseFromString(responseText,'text/html');
            const container = document.getElementById("appendContainer");
            const content = html.querySelector("body")
            console.log(content);
            container.innerHTML = content.innerHTML;
        })
    }
</script>


